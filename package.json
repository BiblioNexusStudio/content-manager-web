{
    "name": "content-manager-web",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "test": "vitest",
        "lint:unused-translations": "run-script-os",
        "lint:unused-translations:default": "(eslint --max-warnings 0 --rule \"svelte-translate-check/unused-translations:error\" \"src/lib/i18n/locales/*.json\" ; ESLINT_STATUS=$? ; rm -f tmp/all-translate-calls.txt ; exit $ESLINT_STATUS)",
        "lint:unused-translations:win32": "eslint --max-warnings 0 --rule \"svelte-translate-check/unused-translations:error\" \"src/lib/i18n/locales/*.json\" & SET ESLINT_STATUS=%ERRORLEVEL%  & IF EXIST tmp\\all-translate-calls.txt (del tmp\\all-translate-calls.txt) & EXIT /B %ESLINT_STATUS%",
        "lint": "yarn format && eslint --max-warnings 0 --fix . && yarn lint:unused-translations && npx svelte-check",
        "lint:ci": "prettier --check \"**/*.{json,js,ts,css,html,svelte,cjs}\" && eslint --max-warnings 0 . && yarn lint:unused-translations && yarn svelte-check",
        "format": "prettier --write \"**/*.{json,js,ts,css,html,svelte,cjs}\"",
        "use-config": "node scripts/use-config.js",
        "prepare": "husky install"
    },
    "dependencies": {
        "@auth0/auth0-spa-js": "^2.1.2",
        "@microsoft/applicationinsights-web": "^3.0.2",
        "@sveltejs/kit": "^1.29.1",
        "@tailwindcss/typography": "^0.5.15",
        "@types/uuid": "^9.0.8",
        "aquifer-tiptap": "https://github.com/BiblioNexusStudio/aquifer-tiptap#1.0.11",
        "chart.js": "^4.4.0",
        "daisyui": "^4.6.0",
        "htmldiff-ts": "https://github.com/BiblioNexusStudio/htmldiff-ts#1.0.6",
        "svelte": "^4.2.19",
        "svelte-awesome": "^3.2.1",
        "svelte-i18n": "^3.7.0",
        "tailwindcss": "^3.4.13",
        "uuid": "^9.0.1"
    },
    "resolutions": {
        "prosemirror-model": "1.22.3"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.16.5",
        "@typescript-eslint/eslint-plugin": "^5.45.0",
        "@typescript-eslint/parser": "^5.45.0",
        "autoprefixer": "^10.4.14",
        "eslint": "^8.28.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-svelte": "^2.43.0",
        "eslint-plugin-svelte-translate-check": "https://github.com/BiblioNexusStudio/eslint-plugin-svelte-translate-check",
        "husky": "^8.0.3",
        "jsdom": "^22.1.0",
        "postcss": "^8.4.31",
        "prettier": "^3.0.3",
        "prettier-plugin-svelte": "^3.0.3",
        "prettier-plugin-tailwindcss": "0.6.8",
        "run-script-os": "^1.1.6",
        "svelte-adapter-azure-swa": "^0.19.0",
        "svelte-check": "^3.4.3",
        "svelte-eslint-parser": "^0.41.0",
        "tslib": "^2.4.1",
        "typescript": "^5.0.0",
        "vite": "^4.5.5",
        "vitest": "^0.32.2"
    },
    "type": "module"
}
